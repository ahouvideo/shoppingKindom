@import "./styles/iconfont.wxss";
/* @import "./styles/icons.wxss"; */
/* 小程序不支持通配符 */

page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
/* 统一主题颜色 通过变量来实现 和字体大小 设计稿375px*/
page{
  --themeColor:#5485a3;
  /* 统一字体大小 1px=2rpx */
  font-size: 36rpx;
}
.clearfix::after{
  content: "";
  display: block;
  clear: both;
}
navigator{
  width: 100%;
  height: auto;
  
}
image{
  width: 100%;
  display: block;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.one-text{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
/* 多行显示 */
.more-text{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
